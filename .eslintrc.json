{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": "off",
    "no-lone-blocks": "off",
    "linebreak-style": "off",
    "max-len": "off",
    "no-case-declarations": "off",
    "default-param-last": "off",
    "eqeqeq": "off"
  }
}
